{
  "title": "Gitbook 指南",
  "language": "zh-hans",
  "author": "zha0cai",
  "links": {
    "sidebar": {
      "My github": "https://github.com/zha0cai"
    }
  },
  "plugins": [
    "-lunr",
    "-search",
    "highlight",
    "search-pro",
    "code",
    "theme-comscore",
    "-splitter",
    "-summary",
    "anchor-navigation-ex",
    "expandable-chapters",
    "versions-select",
    "ace",
    "chart",
    "graph",
    "katex"
  ],

  "pluginsConfig": {
    "anchor-navigation-ex": {
      "showLevel": false,
      "showGoTop": true
    },
    "chart": {
      "type": "c3"
    },
    "sitemap-general": {
      "prefix": "https://zha0cai.github.io/gitbookPublic/home/"
    },
    "my-toolbar": {
      "buttons": [
        {
          "label": "仓库",
          "icon": "fa fa-github",
          "url": "https://github.com/zha0cai/gitbookPublic",
          "position":"right",
          "text": "仓库",
          "target": "_blank"
        }
      ]
    },
    "versions": {
      "gitbookConfigURL": "book.json",
      "options": [
        {
          "value": "https://zha0cai.github.io/gitbookPublic/gitbook-guide/",
          "text": "Gitbook & Honkit 指南",
          "selected": true
        },
        {
          "value": "https://zha0cai.github.io/gitbookPublic/home/",
          "text": "Home Page",
          "selected": true
        }
      ]
    }
  },
  "structure": {
    "readme": "home.md"
  }
}